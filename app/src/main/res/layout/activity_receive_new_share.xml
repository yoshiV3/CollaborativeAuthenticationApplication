<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".alternative.management.extend.ReceiveNewShareActivity">

    <TextView
        android:id="@+id/textView_status"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_applicationName_extend"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="64dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_status"
        tools:layout_editor_absoluteX="61dp" />

    <Button
        android:id="@+id/button_start_extend"
        android:layout_width="162dp"
        android:layout_height="177dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="152dp"
        android:onClick="onCLick"
        android:text="@string/ok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_applicationName_extend" />
</androidx.constraintlayout.widget.ConstraintLayout>